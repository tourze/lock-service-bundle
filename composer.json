{
  "name": "tourze/lock-service-bundle",
  "description": "Lock Service",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/console": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/lock": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/service-contracts": "^3",
    "symfony/yaml": "^6.4 || ^7.1",
    "psr/log": "^3|^2|^1",
    "snc/redis-bundle": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\LockServiceBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\LockServiceBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1",
    "maglnet/composer-require-checker": "^4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}